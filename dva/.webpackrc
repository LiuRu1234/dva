{
  "entry": "src/index.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        ["import", { "libraryName": "antd", "style": "css" }]
      ],
      "sass": true,
      "proxy": {
        "/": {
          "target": "http://www.uxinyue.com:81/api",
          "changeOrigin": true,
          "pathRewrite": { "^/api" : "" }
        }
      },
      "disableCSSModules": true
    },
    "production": {
      "extraBabelPlugins": [
        ["import", { "libraryName": "antd", "style": "css" }]
      ],
      "sass": true,
      "outputPath": "./dist",
      "publicPath": "./",
      "disableCSSModules": true
    }
  }
}
